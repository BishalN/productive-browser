{
  "name": "productive-browser",
  "version": "2.0.0",
  "description": "The most Productive Chrome extension ever",
  "scripts": {
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension --no-content-hash --no-cache",
    "build": "parcel build src/manifest.json --config @parcel/config-webextension --no-content-hash --no-cache",
    "clean": "rm -r .parcel-cache dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bishaln/productive-browser.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/bishaln/productive-browser#readme",
  "devDependencies": {
    "@parcel/config-webextension": "^2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "@types/chrome": "^0.0.216",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.8",
    "parcel": "^2.8.3",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "process": "^0.11.10",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.372.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.0"
  }
}
