{
  "manifest_version": 3,
  "version": "1.0.2",
  "name": "Productive Browser",
  "description": "The most productive Chrome extension ever",
  "background": {
    "service_worker": "background/background.ts",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options/index.html"
  },
  "incognito": "spanning",
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon32.png",
    "64": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "key": "",

  "permissions": ["storage", "identity", "identity.email"],
  "oauth2": {
    "client_id": "987445278418-s4iq6mk2c0p01mmanggqhrb0sia30b9e.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
